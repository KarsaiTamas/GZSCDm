Actor MutaliskLeftSide:GibletsNoBounce
{
	scale 0.8
	states
	{
	
	Spawn:
	MUTR A 0
	
	MUTL ABCDEF 2
	goto Death
	
	Death:
	MUTL G -1
	stop
	}
}

